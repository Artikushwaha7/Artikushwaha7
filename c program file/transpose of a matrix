#include<stdio.h>
#include<conio.h>
void main()
{
    int a[3][3],b[3][3],m,n,i,j;
    printf("\n enter the number of rows and columns");
    scanf("%d%d",&m,&n);
    printf("\n enter elements of the matrix");
    for(i=0;i<m;i++)
    for(j=0;j<n;j++)
    scanf("%d",&a[3][3]);
    for(i=0;i<m;i++)
    for(j=0;j<n;j++)
    b [j][i]=a[i][j];
    printf("\n transopose of the matrix");
    for(i=0;i<n;i++)
    {
        for(j=0;j<m;j++)
        printf("\n %d ",b[i][j]);
        printf("\n");
    }
    getch();
    
    
}

